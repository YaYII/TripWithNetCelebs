<!--logs.wxml-->
<navigation-bar title="日志" back="{{true}}" color="black" background="#FFF"></navigation-bar>
<view class="container">
  <view class="logs-list">
    <block wx:for="{{logs}}" wx:key="timeStamp" wx:for-item="log">
      <view class="log-item">
        <text class="log-time">{{log.date}}</text>
      </view>
    </block>
  </view>
</view>
